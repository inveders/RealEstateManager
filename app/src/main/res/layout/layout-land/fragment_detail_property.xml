<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android..com/tools"
    android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_rootview"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://schemas.android..com/tools ">

    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_image"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_margin="@dimen/default_margin"
        android:adjustViewBounds="true"
        android:contentDescription="@string/description_image"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/actionModeSplitBackground" />

    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_surface_area_image"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_surface_area_title"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_surface_area_title"
        app:srcCompat="@drawable/detail_icon_surface_24dp" />

    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_bedroom_image"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_title"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_title"
        app:srcCompat="@drawable/detail_icon_bedrooms_24dp" />


    <!-- BLOC CARACTERISTIQUE SURFACE -->
    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_room_image"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="70dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_surface_area_title"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_surface_area_title"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_surface_area_title"
        app:srcCompat="@drawable/detail_icon_rooms_24dp" />

    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_bathroom_image"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_number_of_bathroom_title"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_room_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_number_of_bathroom_title"
        app:srcCompat="@drawable/detail_icon_bathrooms_24dp" />

    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_location_image"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_location_title"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_location_title"
        app:srcCompat="@drawable/detail_icon_location_24dp" />

    <!-- BLOC CARACTERISTIQUE NUMBER OF BEDROOM -->
    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_location_map"
        android:layout_width="170dp"
        android:layout_height="100dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_location_text"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_bathroom_image"
        app:layout_constraintRight_toRightOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_location_title"
        app:srcCompat="@color/colorPrimaryDark" />

    <ImageView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_points_of_interest_image"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@string/description_image"
        app:layout_constraintBottom_toBottomOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:srcCompat="@drawable/detail_icon_point_of_interest_24dp" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_description_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_description_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_image" />

    <!-- BLOC CARACTERISTIQUE NUMBER OF ROOMS -->

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_description_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:paddingBottom="15dp"
        android:text="@string/detail_property_description_texte"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintRight_toRightOf="@realEstateAgentId/fragment_detail_property_image"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_description_title" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_surface_area_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="@string/detail_property_surface_area_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_surface_area_image"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_description_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_surface_area_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_surface_area_text"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_surface_area_image"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_surface_area_title" />

    <!-- BLOC CARACTERISTIQUE NUMBER OF BATHROOM -->

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_bedroom_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="65dp"
        android:text="@string/detail_property_number_of_bedroom_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_surface_area_image"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_surface_area_title_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_bedroom_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_number_of_bedroom_text"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_surface_area_image"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_title" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_room_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="@string/detail_property_number_of_room_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_number_of_room_image"

        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_description_text"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_surface_area_title" />


    <!-- BLOC CARACTERISTIQUE LOCATION -->
    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_room_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_number_of_room_text"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_room_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_surface_area_title" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_bathroom_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/detail_property_number_of_bathroom_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_room_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_description_text"

        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_title" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_number_of_bathroom_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_number_of_bathroom_text"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_bathroom_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_number_of_bathroom_title" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_location_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="@string/detail_property_location_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_text"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_title" />


    <!-- BLOC CARACTERISTIQUE POINT OF INTEREST -->
    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_location_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_location_text"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_location_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_location_title" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_points_of_interest_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_points_of_interest_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_number_of_bedroom_text"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_location_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_points_of_interest_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/detail_property_points_of_interest_text"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title" />


    <!-- BLOC CARACTERISTIQUE REAL ESTATE AGENT -->

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_real_estate_agent_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/detail_property_real_estate_agent_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_points_of_interest_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_real_estate_agent_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/detail_property_real_estate_agent_text"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_real_estate_agent_title"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_real_estate_agent_title" />

    <!-- BLOC CARACTERISTIQUE DATE OF ENTRY ON MARKET -->

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_date_of_entry_on_market_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:text="@string/detail_property_date_of_entry_on_market_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_real_estate_agent_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_date_of_entry_on_market_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/detail_property_date_of_entry_on_market_text"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_date_of_entry_on_market_title"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_date_of_entry_on_market_title" />

    <!-- BLOC CARACTERISTIQUE STATUS -->

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_status_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:text="@string/detail_property_status_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_date_of_entry_on_market_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_status_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/detail_property_status_text"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_status_title"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_status_title" />

    <!-- BLOC CARACTERISTIQUE DATE OF SALE -->

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_date_of_sale_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:text="@string/detail_property_date_of_sale_title"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@realEstateAgentId/fragment_detail_property_points_of_interest_title"
        app:layout_constraintTop_toBottomOf="@realEstateAgentId/fragment_detail_property_status_text" />

    <TextView
        android:realEstateAgentId="@+realEstateAgentId/fragment_detail_property_date_of_sale_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/detail_property_date_of_sale_text"
        app:layout_constraintLeft_toRightOf="@realEstateAgentId/fragment_detail_property_date_of_sale_title"
        app:layout_constraintTop_toTopOf="@realEstateAgentId/fragment_detail_property_date_of_sale_title" />


</androidx.constraintlayout.widget.ConstraintLayout>